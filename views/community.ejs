<%- include('./partials/header') %>


    <section class="page">
        <div class="pfeedback">
            <div class="myprofile">
                <div class="dp">
                    <img class="logoprofile" src="/img/dp.png">
                </div>
                <a href="/myprofile" class="button">My Profile </a>
            </div>
            <div class="Suggest">
                Do you want to Suggest a New interest?
            </div>
            <div class="gfeedback">
                <form action="/createinterest?_method=POST" method="POST">
                    <span class="title">Interest Name</span><br>
                    <input type="text" class="title-box" name="title" required>
                    <br><br>
                    <textarea class="textarea" rows="10" placeholder="Give your Feedback" cols="20" name="body"></textarea>
                    <button type="submit" class="submit"><span class="submit_text">Submit</span></button>

                    <!-- <input class="submit" type="submit"></input> -->
                </form>
            </div>
        </div>
        <div class="feedbacks">

            <% allsuggestion.forEach((interest) =>{ %>
                <div>
                    <p> <%= interest.title %> </p>
                    <br>
                    <p> <%= interest.body %> </p>
                    <form action="/liked/<%=interest._id%>?_method=POST" method="POST"><button class="button"">Like</button></form>

                    <!-- <button class="button">like</button> -->
                </div>
            <%})%>
        </div>

    </section>
</body>

</html>