<%- include('./partials/header') %>

<h1 class="ongoing ">Your Events</h1>
<br>
    <section class="events">
        <% events.forEach((event) => { %>
            <div class="box">
                <h1 class="event">
                    <%= event.name %>
                </h1>
                <p class="time">
                    <%= event.startAt %> - <%= event.endAt %>
                </p>
                <p class="time">
                    <%= event.location %>
                </p>
                <p class = "participants">
                Participants : <%= event.members.length %>
                </p>
            </div>
        <% }) %>
    </section>
    <footer>

    </footer>
</body>
</html>